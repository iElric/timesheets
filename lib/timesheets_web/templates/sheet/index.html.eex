<h1>Select a Sheet</h1>

<%= form_for @conn, Routes.sheet_path(@conn, :approve), fn f -> %>
  <div class="form_group">
    <%= label f, :choose_a_woker %>
    <%= select f, :worker, @worker_names, class: "form_control" %>
    <br>
    <%= label f, :choose_a_date %>
    <%= select f, :date, @dates, class: "form_control" %>
  </div>
  
  <div>
    <%= submit "Submit", class: "btn btn-primary" %>
  </div>
<%end%>
